<http://resource.geosciml.org/classifier/cgi/lithology/metasomatic_rock> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/lithology/composite_genesis_rock> ;
    skos:definition "Rock that has fabric and composition indicating open-system mineralogical and chemical changes in response to interaction with a fluid phase, typically water rich."@en ;
    skos:example "aceite"@en,
        "argillisite"@en,
        "beresite"@en,
        "endoskarn"@en,
        "fenite"@en,
        "greisen"@en,
        "gumbeite"@en,
        "propylite"@en,
        "rodingite"@en,
        "spilite"@en ;
    skos:inScheme <http://resource.geosciml.org/classifierScheme/cgi/2016.01/simplelithology> ;
